generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Post {
  id        String   @id @default(cuid())
  slug      String   @unique
  title     String
  content   String
  summary   String?
  repo      String?
  commits   String[]
  tags      String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("posts")
}
